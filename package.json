{
  "name": "triple-c-collective",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently -n pb,next -c green,magenta \"npm:pocketbase:run\" \"next dev\"",
    "build": "next build",
    "build:dev": "concurrently -n pb,next-builder --kill-others \"npm:pocketbase:run\" \"next build\"",
    "start": "next start",
    "preview": "concurrently -n pb,next -c green,cyan \"npm:pocketbase:run\" \"next start\"",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "pocketbase:build": "make -C pocketbase build",
    "pocketbase:run": "make -C pocketbase all",
    "pocketbase:clean": "make -C pocketbase clean",
    "pocketbase:admin": "open http://127.0.0.1:8090/_/",
    "lighthouse:run": "node lighthouse/js/run-reports.mjs",
    "lighthouse:serve": "serve ./lighthouse/js/reports",
    "lighthouse": "npm run lighthouse:run && npm run lighthouse:serve"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx,jsx}": "eslint",
    "*.{js,jsx,ts,tsx,css,md}": "prettier --write"
  },
  "dependencies": {
    "@fireworks-js/react": "^2.10.8",
    "@next/third-parties": "^14.0.4",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-toast": "^1.1.5",
    "@react-email/html": "0.0.7",
    "@react-email/render": "^0.0.10",
    "@react-email/text": "0.0.7",
    "@smastrom/react-rating": "^1.4.0",
    "@vercel/analytics": "^1.2.2",
    "@vercel/speed-insights": "^1.0.12",
    "chrome-launcher": "^1.1.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "embla-carousel-react": "^8.0.0-rc19",
    "husky": "^8.0.3",
    "lighthouse": "^12.2.0",
    "lucide-react": "^0.307.0",
    "next": "13.5.5",
    "nextjs-toploader": "^1.6.6",
    "nodemailer": "^6.9.8",
    "react": "^18",
    "react-dom": "^18",
    "react-icons": "^5.1.0",
    "react-photo-view": "^1.2.4",
    "react-slick": "^0.30.2",
    "react-snowfall": "^2.2.0",
    "react-sparkle": "^2.0.0",
    "slick-carousel": "^1.8.1",
    "tailwind-merge": "^2.2.0"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.10",
    "@testing-library/react": "^14.2.2",
    "@types/escape-html": "^1.0.4",
    "@types/node": "^20",
    "@types/nodemailer": "^6.4.14",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-slick": "^0.23.13",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.0.1",
    "concurrently": "^8.2.2",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.0",
    "postcss": "^8",
    "prettier": "^3.2.2",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "vitest": "^1.4.0"
  }
}
